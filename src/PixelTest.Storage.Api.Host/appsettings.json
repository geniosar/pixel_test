{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "Using": [ "Serilog.Sinks.Console" ],
        "WriteTo": [
            { "Name": "Console" }
        ]
    },
    "AllowedHosts": "*",
    "ASPNETCORE_ENVIRONMENT": "development",
    "URLS": "http://*:2000;https://*:2001;",
    "DATA_FILE_PATH": "data.txt",
    "SAVE_DATA_DELAY": "1"
}
