{
    "AllowedHosts": "*",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "Using": [ "Serilog.Sinks.Console" ],
        "WriteTo": [
            { "Name": "Console" }
        ]
    },
    "ASPNETCORE_ENVIRONMENT": "development",
    "URLS": "http://*:1000;https://*:1001;",
    "STORAGE_HOST": "https://localhost:2001"
}
